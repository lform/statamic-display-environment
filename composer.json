{
    "name": "lform/statamic-display-environment",
    "autoload": {
        "psr-4": {
            "Lform\\EnvDisplay\\": "src"
        }
    },
    "require": {
        "statamic/cms": "^5.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Display Environment",
            "description": "Env Display Addon For Statamic Control Panel"
        },
        "laravel": {
            "providers": [
                "Lform\\EnvDisplay\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
